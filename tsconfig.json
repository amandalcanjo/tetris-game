{
	"compilerOptions": {

		"target": "ES2020",
		"module": "ES2020",
		"lib": [
			"DOM",
			"ES2020"
		],
		"sourceMap": true,
		"outDir": "./dist" ,
		"rootDir": "./src",
		"noEmitOnError": true,
		"strict": true ,
		"noImplicitAny": true ,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
